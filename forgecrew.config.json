{
  "version": 1,
  "project": {
    "name": "Photo-Export-Fixer",
    "owner": "Dave (BigDog)",
    "github": {
      "org": "CrackinLLC",
      "repo": "Photo-Export-Fixer"
    }
  },
  "setup": {
    "commands": [
      "pip install -r requirements.txt"
    ],
